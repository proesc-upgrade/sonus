{
    "name": "closca/sonus",
    "description": "A laravel audio and video conversion, thumbnail generator and metadata editor package powered by ffmpeg",
    "keywords": ["laravel", "ffmpeg", "ffprobe", "converter"],
    "license": "MIT",
    "authors": [
        {
            "name": "Rafael Sampaio",
            "email": "rafaelsampaio@live.com"
        },
        {
            "name": "Closca Costel",
            "email": "closca.costel@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "autoload": {
        "classmap": [
            "src/migrations"
        ],
        "psr-4": {
            "Closca\\Sonus\\": "src/Closca/Sonus/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Closca\\Sonus\\SonusServiceProvider"
            ],
            "aliases": {
                "Sonus": "Closca\\Sonus\\Facade"
            }
        }
    },
    "minimum-stability": "dev"
}
